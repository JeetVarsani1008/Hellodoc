@model DAL.ViewModel.EncounterVm;@{    ViewData["Title"] = "EncounterForm";
    Layout = "~/Views/Shared/Provider/_LayoutProviderDashboard.cshtml";}<div class="container-fluid">    <div class="row ">        <div class="col-lg-2"></div>        <div class="col-lg-8 col-md-12 col-12  ">            <div class="justify-content-between d-flex  my-3 ">                <h4>Encounter Form</h4>                    <button type="button" class=" btn btn-outline-info " onclick="window.location.href='@Url.Action("ProviderDashboard","Provider")'">                        <i class="bi bi-chevron-left"></i>Back                    </button>            </div>            <div class=" d-flex flex-column shadow">                <div class="d-flex justify-content-center mt-3 head">                    <h2>Medical Report-Confidential</h2>                </div>                <form asp-action="PostEncounterData" asp-controller="Provider" method="post" class="p-4" enctype="multipart/form-data">                    <input hidden asp-for="RequestId" />                    <div class="row ">                        <input type="hidden" value="RequestId" name="RequestId" />                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input asp-for="FirstName" type="text" class="form-control" placeholder="" disabled>                                <label for=""> First Name</label>                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input asp-for="LastName" type="text" class="form-control" placeholder="" disabled>                                <label for=""> Last Name</label>                            </div>                        </div>                        <div class=" mb-3">                            <div class="form-floating">                                <input asp-for="Address" type="textarea" class="form-control" placeholder="" disabled>                                <label for=""> Location</label>                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input type="date" class="form-control" placeholder="" disabled>                                <label for=""> Date of Birth</label>                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input type="date" class="form-control" placeholder="" disabled>                                <label for=""> Date</label>                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input asp-for="PhoneNumber" type="tel" class="form-control" id="phone" placeholder="" style="height: 60px !important; ">                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input asp-for="Email" type="email" class="form-control" id="Emailforlogin" onBlur="checkemailexists()" placeholder="">                                <label for="Emailforlogin"> E-mail </label>                                <span asp-validation-for="Email" class="text-danger"></span>                            </div>                        </div>                        <div class="col-sm-6 ">                            <div class="form-floating ">                                <input type="text" class="form-control mb-3" placeholder="">                                <label for=""> History Of Preset Illness Or Injury</label>                         