@model DAL.ViewModel.PartnersVm;@{    Layout = "~/Views/Shared/_LayoutMainAdminDashboard.cshtml";}<main class="bg-light container">    <h4>Vendor(s)</h4>    <div class="d-flex flex-column">        <div class="d-flex align-items-center mt-3 mb-3 me-2 row">            <div class="d-flex outline mt-3 col-sm-12 col-md-2 pe-0">@*                  <span class="searchspan d-flex align-items-center me-1 ms-1">                    <i class="bi bi-search"></i>                </span>  *@                <input id="inputsearch" type="search" class="form-control border-1 pt-3 pb-3 mb-0 search " placeholder="Search Business Name" oninput="AjaxAll()"/>            </div>            <div class="col-sm-12 col-md-3 pe-0">
                <select class="form-select mt-3 Profession w-100" id="selectedProfession" onchange="AjaxAll()">                    <option selected value="0">All Profession</option>                    @foreach(var obj in Model.healthProfessionalType)
                    {
                        <option value="@obj.HealthProfessionalId">@obj.ProfessionName</option>  
                    }                </select>            </div>            <div class="col"></div>            <div class="col-md-2 d-flex justify-content-end pe-0">
                <button type="button" class="btn bg-info ms-auto mt-2" onclick="window.location.href='@Url.Action("AddBusiness","Admin")'">                    <Span class=" d-none d-md-block">                        Add Business                    </Span><i class="bi bi-plus-lg d-block d-md-none"></i>                </button>            </div>        </div>        <div id="partnerstable">
        </div>    </div></main><script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script><script>    //this ajax is call when this page is load    $(document).ready(function () {        $.ajax({            url: "/Admin/ProfessionTable",            type: "POST",            dataType: "html",            success: function (data) {                $("#partnerstable").html(data);            },            error: function () {                $("#partnerstable").html('An error has occurred');            }        });    });    // $("#selectedProfession").on("input", function () {    //     professionId = $(this).val();    //     console.log(professionId);    //     AjaxAll();    // });    function AjaxAll() {        var professionId = $('#selectedProfession').val();        var searchvalue = $('#inputsearch').val().trim();        console.log(searchvalue);        $.ajax({            url: "/Admin/ProfessionTable",            type: "POST",            data: { Id: professionId,                    searchvalue : searchvalue,            },            dataType: "html",            success: function (data) {                console.log(data)                $("#partnerstable").html(data);            },            error: function (error) {                $("#partnerstable").html('An error has occurred');            }        });    }</script> 