@model DAL.ViewModel.AccessVm;@{    Layout = "~/Views/Shared/_LayoutMainAdminDashboard.cshtml";}<div class="create-patient-main mx-auto container">    <div class="row mt-5 mb-3">        <div class="d-flex justify-content-between">            <h4>Create Role</h4>            <button class="btn btn-white border-1 border-info text-info rounded-1" onclick="window.history.back()">&lt; &nbspBack</button>        </div>    </div>    <div class="row shadow">        <div class="ps-3">            <form asp-action="CreateRole" asp-controller="Admin">            <h4 class="mt-3 ml-2">Details</h4>            <div class="row g-1">                <div class="form-floating mb-3  col-md-6 p-1">                    <input type="text" class="form-control " id="pi_name" placeholder="username" value="@Model.RoleName" asp-for="RoleName">                    <label>Role Name</label>                    <span class="text-danger mb-3" asp-validation-for="RoleName" id="roleNameError"></span>                </div>                <div class="form-floating mb-3  col-md-6  d-flex p-1">                    <select class="form-select" aria-label="Default select example" id="Selectedrole" onchange="AjaxAll(this.value)" style=" width: 100%; height:57px !important;" asp-for="AccountType">                        <option value="0">All</option>                        @foreach(var obj in Model.aspNetRole)
                        {
                            <option value="@obj.Id">@obj.Name</option>
                        }                    </select>                    <label for="select">Account Type</label>                </div>            </div>                <partial name="Admin/_CreateAccessTable" />
                <div class="d-flex justify-content-end mb-4">                    <button type="submit" class="btn btn-info me-2" id="save-button" disabled>Save</button>                    <button  type="button" class="btn btn-outline-info" id="">Cancel</button>                </div>
            </form>        </div>    </div></div><script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script><script>    // $("#Selectedrole").on("change", function () {    //     Id = $(this).val();    //     AjaxAll();    // });    function AjaxAll(Id) {        console.log(Id);        $.ajax({            url: "/Admin/fetchrole",            type: "GET",            data: { Id: Id },            dataType: "html",            success: function (data) {                $("#roles").html(data);                Swal.fire({                    position: "top-end",                    icon: "success",                    title: "Your work has been saved",                    showConfirmButton: false,                    timer: 1500                });            },            error: function (error) {                $("#roles").html('An error has occurred');            }        });    }    //if all is selected then disable save button        document.addEventListener('DOMContentLoaded', function () {            var selectedRole = document.getElementById("Selectedrole");            var saveButton = document.getElementById("save-button");            selectedRole.addEventListener('change', function () {                if (selectedRole.value === '0') {                saveButton.disabled = true;            } else {                saveButton.disabled = false;            }        });    });    $(document).ready(function () {        // Handle the input change event        $("#pi_name").change(function () {            // Get the role name            var roleName = $(this).val();            // Send an AJAX request to the server            $.ajax({                url: "/roles/check-role-name",                method: "POST",                data: { roleName: roleName },                success: function (response) {                    // Check if the role name already exists                    if (response.exists) {                        // Show the error message                        $("#roleNameError").text("Role name is already exists.");                    } else {                        // Clear the error message                        $("#roleNameError").text("");                    }                }            });        });    });</script>@section Scripts {    @{        <partial name="_ValidationScriptsPartial" />    }}