@model DAL.ViewModel.AdminOrderVm;@{    Layout = "~/Views/Shared/_LayoutMainAdminDashboard.cshtml";}<head>    <style>        .form-floating > .form-select{        padding-top: 0.8rem;        padding-bottom: 0.625rem;        }    </style>
</head><form method="post" enctype="multipart/form-data">    <input type="text" value="@Model.RequestId" hidden/>    <input type="text" hidden value="" id="vendorId" asp-for="VendorId"/>    <div class="profile-main bg-light">        <div class="container pb-4">              <div class="form-profile ">                <div class="d-flex justify-content-between align-items-center">                <h4 class="m-4 pt-3">Send Orders</h4>                    <a asp-controller="Admin" asp-action="AdminDashboard" class="btn">                        <span class="border border-info text-info p-2 rounded-2">                            < Back                        </span>                    </a>                </div>                <div class="bg-white pt-5">                    <div class="row pb-4 px-4">                        <div class="form-floating mb-3 col-md-6 col-sm-12">                            <select class="form-select w-100" id="healthprofessionSelect" aria-label="Floating label select example">                                <option selected>Select Profession</option>                                @foreach (var obj in Model.healthProfessionType)
                                {                                    <option value="@obj.HealthProfessionalId">                                        @obj.ProfessionName                                    </option>                                }                            </select>                        </div>                        <div class="form-floating mb-3 col-md-6 col-sm-12">                            <select class="form-select w-100" id="businessSelect" aria-label="Floating label select example" placeholder="Business">                                <option selected>Business</option>                            </select>                            <span class="text-danger" asp-validation-for="Business"></span>                        </div>                        <div class="form-floating mb-3 col-md-6 col-sm-12">                            <input type="text" class="form-control" id="businessContact" placeholder="Business Contact" asp-for="BusinessContact">                            <label for="floatingInput" style="margin-left: 1em">Business Contact</label>                            <span class="text-danger" asp-validation-for="BusinessContact"></span>                        </div>                        <div class="form-floating mb-3 col-md-6 col-sm-12">                            <input type="email" class="form-control" id="email" placeholder="Email" asp-for="Email">                            <label for="floatingInput" style="margin-left: 1em">Email</label>                            <span class="text-danger" asp-validation-for="Email"></span>                        </div>                        <div class="form-floating mb-3 col-md-6 col-sm-12">                            <input type="text" class="form-control" id="faxNumber" placeholder="Fax Number" asp-for="FaxNumber">                            <label for="floatingInput" style="margin-left: 1em">Fax Number</label>                        </div>                        <div class=" col-12 form-floating mb-3 ">                           <textarea class="form-control h-100 dclass" id="floatingInput" placeholder=" " rows="5" asp-for="Prescription"></textarea>                           <label for="floatingInput" class="text-wrap ms-2 floating-label">Prescription or Order details</label>                        </div>                        <div class="form-floating mb-3 col-md-6 col-sm-12">                            <select class="form-select w-100" id="" aria-label="Floating label select example" asp-for="Refill">                               <option selected>Number Of Refill</option>                               <option value="1">1</option>                               <option value="2">2</option>                               <option value="3">3</option>                            </select>                        </div>                    </div>                    <div class="d-flex justify-content-end gap-3">                        <button type="submit" id="submitBtn" class="btn btn-info text-white EditBtn">Submit </button>                        <button type="button" id="cancelBtn" class="btn btn-outline-info EditBtn me-4">Cancel</button>                    </div>                </div>                </div>            </div>        </div></form><script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script><script>    //this ajax is for display vendor name when select on profession    $("#healthprofessionSelect").on("change", function () {        getBusinessByHealthProfessionId($(this).val());    })    function getBusinessByHealthProfessionId(healthid) {        // alert("hello");        // console.log(healthid)        let action = "/Admin" + "/" + "BusinessSelect"        event.preventDefault();        $.ajax({            url: action,            type: 'GET',            data: { healthProfessionId: healthid },            success: function (data) {                console.log(data.adminOrderVm.healthProfessionals)                $('#businessSelect').html("<option>" + "Select Business" + "</option>");                for (var i = 0; i < data.adminOrderVm.healthProfessionals.length; i++) {                    $("#businessSelect").append('<option value="' + data.adminOrderVm.healthProfessionals[i].vendorId + '">' + data.adminOrderVm.healthProfessionals[i].vendorName + '</option>');                };            },            error: function () {                alert('Error loading partial view');            },        });    }    //this ajax is for display data of vendor    $('#businessSelect').change(function () {        var VendorId = $(this).val();        console.log(VendorId)        $.ajax({            url: '@Url.Action("GetVendorDetails", "Admin")',            type: 'GET',            data: { VendorId: VendorId },            success: function (data) {                $.each(data, function (index, item) {                    $('#businessContact').val(item.businessContact);                    $('#vendorId').val(item.vendorId);                    $('#email').val(item.email);                    $('#faxNumber').val(item.faxNumber);                })            }        });    });</script>@section Scripts {    @{        <partial name="_ValidationScriptsPartial" />    }}