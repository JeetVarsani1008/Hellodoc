@model DAL.ViewModel.RecordVm;@{	Layout = "~/Views/Shared/_LayoutMainAdminDashboard.cshtml";}<div class="mt-5 container px-4">	<div class="d-flex justify-content-between align-items-center mb-4">		<h3 class="d-flex justify-content-start">Email Logs (Gmail)</h3>		<button class="btn btn-outline-info">&lt; Back</button>	</div>	<div class="container-fluid p-3 p-0 shadow">		<div class="searchbar">			<div class="row d-flex align-items-center">				@* select requst status *@				<div class="col-lg-2 col-md-2 col-sm-12 form-floating mb-3">					<select class="form-select w-100" id="selectrole">						<option selected value="0">All</option>						@if(Model.roles != null)						{							@foreach (var obj in Model.roles)							{								<option value="@obj.RoleId">@obj.Name</option>								}							}					</select>					<label class="ms-2">Search By Role</label>				</div>				@* receiver name *@				<div class="col-lg-2 col-md-2 col-sm-12 form-floating">					<input type="text" class="form-control " id="receivername" placeholder="ReceiverName">					<label class="ms-2">Receiver Name</label>				</div>				 @* Email  *@				<div class="col-lg-2 col-md-2 col-sm-12 form-floating">					<input type="text" class="form-control " id="email" placeholder="Email">					<label class="ms-2">Email Id</label>				</div>				@* created date  *@				<div class="col-lg-2 col-md-2 col-sm-12 form-floating">					<input type="date" class="form-control " id="createdate" placeholder="CreatedDate">					<label class="ms-2">Created Date</label>				</div>				@* sent date *@				<div class="col-lg-2 col-md-2 col-sm-12 form-floating">					<input type="date" class="form-control " id="sentdate" placeholder="SentDate">					<label class="ms-2">Sent Date</label>				</div>				<div class="col-lg-2 col-md-2 col-sm-12">					<div class="d-flex justify-content-end">						<button class="btn btn-info text-white me-2" onclick="searchemaillog(0)">Search</button>						<button class="btn btn-outline-info" onclick="clearfxn(0)">Clear</button>					</div>				</div>			</div>		</div>		<div id="emaillog"></div>	</div></div><script src="https://code.jquery.com/jquery-3.7.1.min.js" integrity="sha256-/JqT3SQfawRcv/BIHPThkBvs0OEvtFFmqPF/lYI/Cxo=" crossorigin="anonymous"></script><script>	//this is for pass null date	$("#sentdate").change(function () {		if ($(this).val() == "") {			$(this).val(null);		}	});	//this ajax is call when this page is load	$(document).ready(function () {		$.ajax({			url: "/Admin/EmailLogTable",			type: "POST",			dataType: "html",			success: function (data) {				$("#emaillog").html(data);			},			error: function () {				$("#emaillog").html('An error has occurred');			}		});	});	function searchemaillog(pageNumber) {		var sentdate = $("#sentdate").val();		if (sentdate === "") {			sentdate = null;		}		var receivername = $('#receivername').val().trim();		var email = $('#email').val().trim();		var createdate = $('#createdate').val().trim();		var roleid = $('#selectrole').val();		var pageNumber = pageNumber;		console.log(pageNumber);		$.ajax({			url: "/Admin/EmailLogTable",			type: "POST",			data: {				roleid : roleid,				receivername: receivername,				email : email,				sentdate: sentdate,				createdate: createdate,				PageNumber : pageNumber,			},			dataType: "html",			success: function (data) {				$("#emaillog").html(data);			},			error: function (error) {				$("#emaillog").html('Opps Something went wrong');			}		});	};	function checkAndPaginate(pageNumber) {		if (pageNumber > 0) {			searchemaillog(pageNumber);		}	};	//for clear	function clearfxn(pageNumber) {		var receivername;		var email;		var createddate;		var sentdate;		var roleid;		var pageNumber = pageNumber;		$.ajax({			url: "/Admin/EmailLogTable",			type: "POST",			data: {				receiverName: receivername,				createdDate: createddate,				sentDate: sentdate,				email: email,				pageNumber: pageNumber,				roleId: roleid,			},			dataType: "html",			success: function (data) {				$("#emaillog").html(data);				$("#receivername").val("");				$("#createdate").val("");				$("#sentdate").val("");				$("#email").val("");				$("#roleid").val("");			},			error: function (error) {				$("#emaillog").html('Something Went Wrong');			}		});	};</script>