@model DAL.ViewModel.AdminCreateRequestVm;@{    ViewData["Title"] = "Create Patient Request";    Layout = "~/Views/Shared/Provider/_LayoutProviderDashboard.cshtml";}<head>    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/css/intlTelInput.css" />    <script src="https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/intlTelInput.min.js"></script>    <script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>    <style>        .iti {
            width:100%;
        }
    </style></head><div data-bs-theme="light" id="mainContainer">    <div class="container d-flex" style=" height: calc(100% - 80px);">        <div class="container d-flex  flex-column justify-content-start h-100">            <div class="container d-flex align-items-center justify-content-between mb-3">                <h3 id="loginHeading">Submit Information</h3>                    <button class="btn btn-outline-primary m-2 " type="button" onclick="window.location.href='@Url.Action("AdminDashboard","Admin")'">                        back                    </button>            </div>            <div class="main-content d-flex flex-column  m-lg-5 m-md-2 m-sm-1 " style="margin:0!important">                <form method="post" asp-controller="Admin" asp-action="AdminCreateRequest" class="formBorder" enctype="multipart/form-data">                    <h4 class=" d-flex justify-content-start" style="width: 100%;"><span>Patient Information</span></h4>                    <div class="row " style="z-index:8">                        <div class="col-md-6 mt-3">                            <div class="form-floating">                                <input type="text" class="form-control" placeholder="Name" asp-for="FirstName" />                                <label>First Name</label>                                <span asp-validation-for="FirstName" class="text-danger"></span>                            </div>                        </div>                        <div class="col-md-6 mt-3">                            <div class="form-floating">                                <input type="text" class="form-control" placeholder="Username" asp-for="LastName" />                                <label>Last name</label>                                <span asp-validation-for="LastName" class="text-danger"></span>                            </div>                        </div>                        <div class="col-md-6 mt-3">                            <div class="form-floating  ">                                <input id="phone" type="tel" class="form-control w-100" style="height: 60px; " asp-for="Phone">                                <span asp-validation-for="Phone" class="text-danger"></span>                            </div>                        </div>                        <div class="col-md-6 mt-3">                            <div class="form-floating">                                <input type="text" class="form-control" placeholder="Email" id="AEmail" asp-for="Email" oninput="Checkemail()" />                                <label>Email</label>                                <span asp-validation-for="Email" class="text-danger"></span>                            </div>                            <div id="Status"></div>                        </div>                        <div class="col-md-6 mt-3">                            <div class="form-floating " id="datepicker" style="z-index:1">                                <input type="date" class="form-control" id="date" placeholder="Date" oninput="validateDate()" asp-for="DateOfBirth" value="<%= DateTime.Now.ToString('yyyy-MM-dd') %>" />                                <label for="date" class="col-form-label">date of birth</label>                                <span asp-validation-for="DateOfBirth" class="text-danger"></span>                            </div>                        </div>                        <h4 class="mt-5"> Location</h4>                        <div class="col-md-6 mt-3">                            <div class="form-floating ">                                <input type="text" class="form-control" placeholder="Street" asp-for="Street" />                                <label>Street</label>                                <span asp-validation-for="Street" class="text-danger"></span>                            </div>                        </div>                        <div class="col-md-6 mt-3 inx1">                            <div class="form-floating">                                <input type="text" class="form-control" placeholder="City" asp-for="City" />                                <label>City</label>                                <span asp-validation-for="City" class="text-danger"></span>                            </div>                        </div>                        <div class="col-md-6 mt-3">                            <div class="form-floating">                                <select class="form-select w-100" id="regionselect" aria-label="Floating label select example" asp-for="RegionId">                                    <option value="" selected disabled>All</option>                                    @foreach (var obj in Model.regions)                                    {                                        <option value="@obj.RegionId">                                            @obj.Name                                        </option>                                    }                                </select>                                <label for="regionselect">Select Region</label>                                <span asp-validation-for="RegionId" class="text-danger"></span>                            </div>                        </div>                        <div class="col-md-6 mt-3 inx1">                            <div class="form-floating">                                <input type="text" class="form-control" placeholder="Conatct" asp-for="ZipCode" />                                <label>Zip Code</label>                                <span asp-validation-for="ZipCode" class="text-danger"></span>                            </div>                        </div>                        <div class="col-md-6 mt-3">                            <div class="form-floating">                                <input type="text" class="form-control" placeholder="text" asp-for="Room" />                                <label>Room # suite(optional)</label>                            </div>                        </div>                        <div class="form-floating mt-3  col-sm-12 col-md-2 col-lg-1 " style="height: 100%;">                            <a class="dclass btn btn-outline-dark border-info p-3 text-info w-100" id="Verify"><span class="fs-">Verify</span></a>                        </div>                        <div class="form-floating mt-3  col-sm-12 col-md-2 col-lg-1 " style="height: 100%;">                            <div class="form-floating">                                <a class="dclass btn btn-outline-dark border-info ps-1 pe-1 pb-3 pt-3 text-info w-100" id="editBtn">                                    <i class="bi bi-geo-alt-fill"></i>                                    <span class="fs-">Map</span>                                </a>                                </a>                            </div>                        </div>                        <h4 class="mt-5">                            Notes                        </h4>                        <div class="form-floating col-12 mt-3">                            <textarea class="form-control" style="height: 140%" placeholder="Leave a comment here" asp-for="Comments" id="floatingTextarea"></textarea>                            <label for="floatingTextarea" class="p-3 ms-2" id="">Admin Notes(Optional)</label>                        </div>                        <div class="d-flex justify-content-end mt-5 finalsubmit mb-5">                            <button class="btn btn-primary mx-3" type="submit">Submit</button>                            <button class="btn btn-outline-primary" type="button">                                Cancel                            </button>                        </div>                    </div>                </form>            </div>        </div>    </div></div><script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script><script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script><script>    const phoneInputField = document.querySelector("#phone");    const phoneInput = window.intlTelInput(phoneInputField, {        utilsScript:            "https://cdnjs.cloudflare.com/ajax/libs/intl-tel-input/17.0.8/js/utils.js",    });</script><script>    function Checkemail() {        $("#Status").html("Checking...");        $.post("@Url.Action("Checkemail", "Provider")",            {                userdata: $("#AEmail").val()            },            function (data) {                if (data == 1) {                    $("#Status").html('<span style="color: green;">User existed</span>')                    $("#AEmail").css("border-color", "green");                }                else {                    $("#Status").html('<span style="color: blue;">User does not exist</span>')                    $("#AEmail").css("border-color", "blue");                }            }        );    }</script><script>    function validateDate() {        var inputDate = document.getElementById("date").value;        var currentDate = new Date().toISOString().split('T')[0];        if (inputDate > currentDate) {            // Reset the date to the current date            document.getElementById("date").value = currentDate;        }    }</script><script>    $(document).ready(function () {        $("#Verify").click(function () {            // Get the state name from the input field (replace 'stateField' with the actual ID or class of your state input)            var stateName = $("#region").val();            console.log("State Name:", stateName);            // Perform an AJAX request to check the state in the database            $.ajax({                url: '/Admin/CheckRegion', // Replace with the actual URL of your controller action                type: 'POST',                data: { stateName: stateName },                dataType: 'json',  // Explicitly set the expected data type                success: function (response) {                    // Update the border color based on the response                    if (response) {                        // State exists, set border to green                        $("#region").css("border-color", "green");                    } else {                        // State doesn't exist, set border to red                        $("#region").css("border-color", "red");                    }                },                error: function () {                    // Handle errors, if needed                    console.log("error")                }            });        });    });</script>