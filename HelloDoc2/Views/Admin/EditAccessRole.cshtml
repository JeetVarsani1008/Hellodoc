@model DAL.ViewModel.AccessVm;@{    Layout = "~/Views/Shared/_LayoutMainAdminDashboard.cshtml";}<head>    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@10">    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script></head><div class="create-patient-main mx-auto container">    <div class="row mt-5 mb-3">        <div class="d-flex justify-content-between">            <h4>Edit Role</h4>            <button class="btn btn-white border-1 border-info text-info rounded-1" onclick="window.history.back()">&lt; &nbspBack</button>        </div>    </div>    <div class="row shadow">        <div class="ps-3">            <form asp-action="EditAccessRolePost" asp-controller="Admin" >                <input hidden asp-for="RoleId" />                <h4 class="mt-3 ml-2">Details</h4>                <div class="row g-1">                    <div class="form-floating mb-3  col-md-6  d-flex p-1">                        <input type="text" class="form-control " id="pi_name" placeholder="username" asp-for="RoleName">                        <label>Role Name</label>                        <span class="text-danger mb-3" asp-validation-for="RoleName"></span>                    </div>                    <div class="form-floating mb-3  col-md-6  d-flex p-1">                        <select class="form-select" aria-label="Default select example" id="Selectedrole" onchange="AjaxAll(this.value)" style=" width: 100%; height:57px !important;" asp-for="AccountType" disabled>                            <option value="0">All</option>                            @foreach (var obj in Model.aspNetRole)
                            {                                <option value="@obj.Id">@obj.Name</option>                            }                        </select>                        <label for="select">Account Type</label>                    </div>                </div>                <main id="roles" class="d-flex flex-wrap form-floating ">                    @foreach (var item in Model.menu)
                    {                        <div class="form-check">                        @if(Model.roleMenu.Count(x => x.MenuId == item.MenuId) > 0)
                        {
                            <input type="checkbox" name="checkboxForAllRole" value="@item.MenuId" class="checkbox-input"  checked/>
                        }                        else
                        {
                            <input type="checkbox" name="checkboxForAllRole" value="@item.MenuId" class="checkbox-input" />
                        }                        <label class="ms-2">@item.Name</label>
                        </div>                    }                </main>                <div class="d-flex justify-content-end mb-4">                    <button type="submit" class="btn btn-info me-2" id="save-button">Save</button>                    <button type="button" class="btn btn-outline-info" id="">Cancel</button>                </div>            </form>        </div>    </div></div><script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script><script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>