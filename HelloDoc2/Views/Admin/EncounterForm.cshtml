mb-3">                            <div class="form-floating ">                                <input asp-for="Heent" type="text" class="form-control" placeholder="">                                <label for=""> Heent</label>                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input asp-for="Cv" type="text" class="form-control" placeholder="">                                <label for=""> CV</label>                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input asp-for="Chest" type="text" class="form-control" placeholder="">                                <label for=""> Chest</label>                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input asp-for="Abd" type="text" class="form-control" placeholder="">                                <label for=""> ABD</label>                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input asp-for="Extr" type="text" class="form-control" placeholder="">                                <label for=""> Extr</label>                            </div>                        </div>                        <div class="col-sm-6 mb-3">                            <div class="form-floating ">                                <input asp-for="Skin" type="text" class="form-control" placeholder="">                                <label for=""> Skin</label>                            </div>                        </div> 