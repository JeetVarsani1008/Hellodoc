<div class="table-responsive">    <table class="table" id="Profession">        <thead class="table-active">            <tr>                <th scope="col">Profession</th>                <th scope="col">Business Name</th>                <th scope="col">Email</th>                <th scope="col">Fax Number</th>                <th scope="col">Phone Number</th>                <th scope="col">Busniess Contact</th>                <th scope="col">Action</th>            </tr>        </thead>        <tbody>            @foreach (var obj in Model.vendor)            {                <tr>                    <td class="text-dark">@obj.Profession</td>                    <td class="text-dark">@obj.BusinessName</td>                    <td class="text-dark">@obj.Email</td>                    <td class="text-dark">@obj.FaxNumber</td>                    <td class="text-dark">@obj.PhoneNumber</td>                    <td class="text-dark">@obj.BusinessContact</td>                    <td class="d-flex">                        <button class="btn btn-outline-info ms-2" onclick="window.location.href='@Url.Action("UpdateBusiness","Admin", new{vendorId = obj.VendorId})'">Edit</button>                        <button class="btn btn-outline-info ms-2" data-bs-target="#DeleteVendorModal_@obj.VendorId" data-bs-toggle="modal" data-vendor-id="@obj.VendorId">Delete</button>                    </td>                </tr>            }        </tbody>    </table></div>@foreach(var obj in Model.vendor)
{    <div class="modal fade" id="DeleteVendorModal_@obj.VendorId" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">        <div class="modal-dialog modal-dialog-centered">            <div class="modal-content">                <div class=" modal-body m-3 text-center">                    <div class="d-flex justify-content-center ">                        <i class="bi bi-exclamation-circle-fill text-warning d-flex justify-content-center fs-1 mb-3 border border-2 border-warning rounded-circle  p-2"></i>                    </div>                    <h5 class="titlee my-3 " style="font-family: 'Montserrat', sans-serif;                      font-weight: 600;">                        Confirmation For Delete Vendor                    </h5>                    <div class=" my-3 mx-4" style=" font-size: 13px;                      font-family: 'Montserrat', sans-serif;">                        Are you sure you want to Delete this Vendor? Once this                        Vendor is Delete then ypu will not be able to see this Vendor.                    </div>                </div>                <form method="post" asp-action="PartnersDeleteVendors" asp-controller="Admin">                    <input type="text" id="VendorId" hidden value="@obj.VendorId" name="vendorId" />                    <div class="m-3 d-flex justify-content-center">                        <button type="submit" class="btn btn-info confirm-btn text-white" data-bs-dismiss="modal">                            Delete                        </button>                        <button type="button" class="btn cancel-btn border-info text-info ms-3" data-bs-dismiss="modal">                            Cancel                        </button>                    </div>                </form>            </div>        </div>    </div>}