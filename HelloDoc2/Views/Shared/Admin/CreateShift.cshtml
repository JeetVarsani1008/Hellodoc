@model DAL.ViewModel.ViewShiftVm;<div class="CreateReqBtn bg-white">    <div class="modal fade" id="CreateShiftContent" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">        <div class="modal-dialog modal-dialog-centered">            <div class="modal-content">                <div class="modal-header bg-info">                    <h1 class="modal-title fs-5" id="exampleModalLabel">Create Shift</h1>                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>                </div>                <form asp-action="CreateShiftPost" asp-controller="Admin" method="post">                    <div class="modal-body mt-2">                        <div class="">                            <select class="form-select ms-0 mb-3 regionDropdown" aria-label="Default select example" id="regionSelect" style="height:58px !important; width: 100%;" asp-for="RegionId">                                 <option value="" selected disabled>Region</option>                                @foreach(var obj in Model.Regions)
                                {
                                    <option value="@obj.RegionId">@obj.Name</option>  
                                }                            </select>                            <select asp-for="PhysicianId" class="form-select ms-0 mb-3" aria-label="Default select example" id="physicianSelectbyregion" style="height:58px !important; width: 100%;">                                <option selected>Select Physician</option>                            </select>                            <span asp-validation-for="PhysicianId" class="text-danger"></span>                        </div>                        <div class="form-floating mb-3">                            <input asp-for="ShiftDate" type="date" class="form-control" id="shiftdate" placeholder="" value="<%= DateTime.Now.ToString(' yyyy-MM-dd') %>" min="<@(DateTime.Now.ToString(" yyyy-MM-dd"))>" />                            <label for="StartDate">ShiftDate</label>                        </div>                        <span asp-validation-for="ShiftDate" class="text-danger"></span>                        <div class="row g-2">                            <div class="col-md-6">                                <div class="form-floating mb-3">                                    <input asp-for="StartTime" type="time" id="StartTime" class="form-control" placeholder="">                                    <label for="StartTime">Start</label>                                </div>                            </div>                            <div class="col-md-6">                                <div class="form-floating mb-3">                                    <input asp-for="EndTime" type="time" class="form-control" id="EndTime" placeholder="">                                    <label for="street">End</label>                                </div>                            </div>                        </div>                        <span asp-validation-for="StartTime" class="text-danger"></span><br />                        <span asp-validation-for="EndTime" class="text-danger"></span>                        <div class="form-check form-switch d-flex align-items-center ps-1">                            <span class="ms-0" style="margin-right:15%">Repeat</span>                            <input asp-for="IsRepeat" class="form-check-input me-1" type="checkbox" role="switch" id="flexSwitchCheckChecked" style="width: 2.7rem !important;">                        </div>                        <div id="repeatContent">                            <div class="mt-3 mb-3">                                <p class="mb-0">Repeat Days</p>                                <div class="row">                                    <div class="col-md-auto mb-1">                                        <input name="WeekDaysList" type="checkbox" class="me-1 mt-0 form-check-input" value="0" />                                        <span class="me-4">Every Sunday</span>                                    </div>                                    <div class="col-md-auto mb-1">                                        <input name="WeekDaysList" type="checkbox" class="me-1 mt-0 form-check-input" value="1" />                                        <span class="me-4">Every Monday</span>                                    </div>                                    <div class="col-md-auto mb-1">                                        <input name="WeekDaysList" type="checkbox" class="me-1 mt-0 form-check-input" value="2" />                                        <span class="me-4">Every Tuesday</span>                                    </div>                                    <div class="col-md-auto mb-1">                                        <input name="WeekDaysList" type="checkbox" class="me-1 mt-0 form-check-input" value="3" />                                        <span class="me-4">Every Wednesday</span>                                    </div>                                    <div class="col-md-auto mb-1">                                        <input name="WeekDaysList" type="checkbox" class="me-1 mt-0 form-check-input" value="4" />                                        <span class="me-4">Every Thursday</span>                                    </div>                                    <div class="col-md-auto mb-1">                                        <input name="WeekDaysList" type="checkbox" class="me-1 mt-0 form-check-input" value="5" />                                        <span class="me-4">Every Friday</span>                                    </div>                                    <div class="col-md-auto mb-1">                                        <input name="WeekDaysList" type="checkbox" class="me-1 mt-0 form-check-input" value="6" />                                        <span class="me-4">Every Saturday</span>                                    </div>                                </div>                            </div>                            <div class="form-floating mb-3">                                <select asp-for="RepeatUpto" class="form-select ms-0" aria-label="Default select example" id="repeatupto" style="height:58px !important; width: 100%;">                                    <option selected disabled>Select Repeat Number</option>                                    <option value="1">One Time</option>                                    <option value="2">Two Time</option>                                    <option value="3">Three Time</option>                                    <option value="4">Four Time</option>                                </select>                                <label for="RepeatUpto">Repeat End</label>                            </div>                        </div>                    </div>                    <div class="modal-footer">                        <button type="submit" class="btn btn-info" id="confirm-btn">Save</button>                        <button type="button" class="btn btn-outline-info btnmodal" data-bs-dismiss="modal" id="cancel-btn">Cancel</button>                    </div>                </form>            </div>        </div>    </div></div><script>    $("#repeatContent").hide();    $("#flexSwitchCheckChecked").change(function () {        if ($(this).is(":checked")) {            $("#repeatContent").show();        } else {            $("#repeatContent").hide();        }    });    $("#regionSelect").on("change", function () {        getPhysiciansByRegionId($(this).val());    })    function getPhysiciansByRegionId(regid) {        let action = "/Admin" + "/" + "GetPhysiciansByRegion"        event.preventDefault();        $.ajax({            url: action,            type: 'GET',            data: { regionId: regid },            success: function (data) {                console.log(data.viewShiftVm.physicianList)                $('#physicianSelectbyregion').html("<option>" + "Select Physician" + "</option>");                for (var i = 0; i < data.viewShiftVm.physicianList.length; i++) {                    $("#physicianSelectbyregion").append('<option value="' + data.viewShiftVm.physicianList[i].physicianId + '">' + data.viewShiftVm.physicianList[i].firstName + '</option>');                };            },            error: function () {                alert('Error loading partial view');            },        });    };    var today = new Date();    var yyyy = today.getFullYear();    var mm = String(today.getMonth() + 1).padStart(2, '0');    var dd = String(today.getDate()).padStart(2, '0');    var formattedDate = yyyy + '-' + mm + '-' + dd;    document.getElementById("shiftdate").setAttribute("min", formattedDate);</script>