<div class="d-none d-md-block table-responsive">	@if(Model.emailLog.Count > 0)
	{
	<table class="table">		<thead>			<tr>				<th scope="col">Receipient</th>				<th scope="col">Action</th>				<th scope="col">Role Name</th>				<th scope="col">Email Id</th>				<th scope="col">Created Date</th>				<th scope="col">Sent Date</th>				<th scope="col">Sent</th>				<th scope="col">Sent Time</th>				<th scope="col">Confirmation Number</th>			</tr>		</thead>		<tbody>			@foreach (var obj in Model.emailLog)			{				<tr>					<td scope="row">@obj.Recipient</td>					<td>@obj.Action</td>					<td>@obj.RoleName</td>					<td>@obj.Email</td>					<td>@obj.CreatedDate</td>					<td>@obj.SentDate</td>					<td>@obj.Sent</td>					<td>@obj.SentTries</td>					<td>@obj.ConfirmationNumber</td>				</tr>			}		</tbody>		</table>
	}	else
	{
		<p>No Data Found Releted Your Search</p>
	}</div>@* accordian *@<div class="d-block d-md-none my-3">	<div class="accordion accordion-flush" id="emailLogAccordion">		@foreach (var item in Model.emailLog)
		{			<div class="accordion-item">				<h2 class="accordion-header" id="heading_@item.EmailLogId">					<button class="accordion-button collapsed rounded-2" type="button" data-bs-toggle="collapse" data-bs-target="#contentaccordian_@item.EmailLogId" aria-expanded="false" aria-controls="content" style="background-color: #E6faff;">						<div class="d-flex justify-content-start align-items-center">							<div>								<p class="mb-0" style="font-weight: 600;">@item.Recipient</p>								<br />								<p class="mb-0" style="color:gray; font-size:12px;">Action Name : <span>@item.Action</span></p>								<p class="mb-0" style="color: gray; font-size: 12px;">Email : <span style="font-weight: 600;">@item.Email</span></p>							</div>						</div>					</button>				</h2>				<div id="contentaccordian_@item.EmailLogId" class="accordion-collapse collapse" style="background:#E6faff" aria-labelledby="heading_@item.EmailLogId" data-bs-parent="#emailLogAccordion">					<div class="accordion-body">						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-person text-info"></i>							<span>Role Name : @item.RoleName</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-calendar-week text-info"></i>							<span>Created Date : @item.CreatedDate</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-calendar-week text-info"></i>							<span>Sent Date : @item.SentDate</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Sent : @item.Sent</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-envelope text-info"></i>							<span>Sent Tries : @item.SentTries</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Confirmation Number : @item.ConfirmationNumber</span>						</div>					</div>				</div>			</div>		}	</div></div>@* accordian end *@<nav aria-label="Page navigation example " class="pb-1">	<div class="justify-content-between d-flex">		@if ((Model.skipCount + Model.Page.itemperpage) >= Model.Page.totalitem)
		{			<p style="margin:0px; color:gray;">@(Model.skipCount + 1) to @Model.Page.totalitem records out of @Model.Page.totalitem</p>		}
		else
		{			<p style="margin:0px; color:gray;">@(Model.skipCount + 1) to @(Model.skipCount + Model.Page.itemperpage) records out of @Model.Page.totalitem</p>		}		<ul class="pagination">			<li class="page-item @(Model.CurrentPage == 1 ? "disabled" : "")" onclick="checkAndPaginate(@(Model.CurrentPage - 1),0)">				<button class="page-link" id="prevPageLink">Previous</button>			</li>			@for (int i = 1; i <= Model.TotalPages; i++)			{				<li class="page-item @(Model.CurrentPage == i ? "active" : "")" onclick="searchemaillog(@i,@Model.Page.itemperpage)">					<button class="page-link" id="pageLink">@i</button>				</li>			}			<li class="page-item @(Model.CurrentPage == Model.TotalPages ? "disabled" : "")" onclick="checkMaxPage(@Model.CurrentPage + 1)">				<button class="page-link" id="nextPageLink">Next</button>			</li>		</ul>		<input type="text" hidden value="@Model.TotalPages" id="maxpages" />	</div></nav><script>	function checkMaxPage(pageNumber) {		var maxpage = document.getElementById("maxpages");		console.log(maxpage);		if (pageNumber < maxpage) {			searchemaillog(pageNumber);		}	}</script>