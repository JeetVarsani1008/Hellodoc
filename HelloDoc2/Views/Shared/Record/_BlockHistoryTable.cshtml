<div class="d-none d-md-block table-responsive">	@if(Model.blockHistory.Count > 0)
	{		<table class="table">			<thead class="table-active">				<tr>					<th>Patient Name</th>					<th>Phone Number</th>					<th>Email</th>					<th>Created Date</th>					<th>Notes</th>					<th class="text-center">IsActive</th>					<th class="d-flex justify-content-center">Action</th>				</tr>			</thead>			<tbody>				@foreach (var obj in Model.blockHistory)
				{					<tr>						<td>@obj.PatientName</td>						<td>@obj.PhoneNumber</td>						<td>@obj.Email</td>						<td>@obj.CreatedDate</td>						<td>@obj.Notes</td>						<td class="text-center">							<input type="checkbox" @(obj.IsActive ? "checked" : "") class="align-self-center" />						</td>						<td class="d-flex justify-content-center">							<button class="btn btn-outline-info" onclick="window.location.href='@Url.Action("UnblockRequest","Admin",new {requestId = obj.RequestId, blockReqId = obj.BlockRequestId})'">Unblock</button>						</td>					</tr>				}			</tbody>		</table>
	}	else
	{
		<p>No Data Found.</p>
	}</div>@* accordian *@<div class="d-block d-md-none my-3">	<div class="accordion accordion-flush" id="blockhistoryAccordion">		@foreach (var item in Model.blockHistory)
		{			<div class="accordion-item">				<h2 class="accordion-header" id="heading_@item.BlockRequestId">					<button class="accordion-button collapsed rounded-2" type="button" data-bs-toggle="collapse" data-bs-target="#contentaccordian_@item.BlockRequestId" aria-expanded="false" aria-controls="content" style="background-color: #E6faff;">						<div class="d-flex justify-content-start align-items-center">							<div>								<p class="mb-0" style="font-weight: 600;">@item.PatientName</p>								<br />								@* <p class="mb-0" style="color:gray; font-size:12px;">Action Name : <span>@item.Action</span></p> *@								<p class="mb-0" style="color: gray; font-size: 12px;">Email : <span style="font-weight: 600;">@item.Email</span></p>							</div>						</div>					</button>				</h2>				<div id="contentaccordian_@item.BlockRequestId" class="accordion-collapse collapse" style="background:#E6faff" aria-labelledby="heading_@item.BlockRequestId" data-bs-parent="#blockhistoryAccordion">					<div class="accordion-body">						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-person text-info"></i>							<span>Phone : @item.PhoneNumber</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-calendar-week text-info"></i>							<span>Created Date : @item.CreatedDate</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-calendar-week text-info"></i>							<span>Notes : @item.Notes</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Is Active : 								@if (item.IsActive == true)
								{
									<span>Yes</span>
								}
								else
								{
									<span>No</span>
								}
							</span>						</div>					</div>				</div>				</div>		}	</div></div>@* accordian end *@<nav aria-label="Page navigation example " class="pb-1">	<div class="justify-content-between d-flex">		@if ((Model.skipCount + Model.Page.itemperpage) >= Model.Page.totalitem)
		{			<p style="margin:0px; color:gray;">@(Model.skipCount + 1) to @Model.Page.totalitem records out of @Model.Page.totalitem</p>		}
		else
		{			<p style="margin:0px; color:gray;">@(Model.skipCount + 1) to @(Model.skipCount + Model.Page.itemperpage) records out of @Model.Page.totalitem</p>		}		<ul class="pagination">			<li class="page-item @(Model.CurrentPage == 1 ? "disabled" : "")" onclick="checkAndPaginate(@(Model.CurrentPage - 1),0)">				<button class="page-link" id="prevPageLink">Previous</button>			</li>			@for (int i = 1; i <= Model.TotalPages; i++)			{				<li class="page-item @(Model.CurrentPage == i ? "active" : "")" onclick="searchblockpatient(@i,@Model.Page.itemperpage)">					<button class="page-link" id="pageLink">@i</button>				</li>			}			<li class="page-item @(Model.CurrentPage == Model.TotalPages ? "disabled" : "")" onclick="checkMaxPage(@Model.CurrentPage + 1)">				<button class="page-link" id="nextPageLink">Next</button>			</li>		</ul>		<input type="text" hidden value="@Model.TotalPages" id="maxpages" />	</div></nav><script>	function checkMaxPage(pageNumber) {		var maxpage = document.getElementById("maxpages");		console.log(maxpage);		if (pageNumber < maxpage) {			searchblockpatient(pageNumber);		}	}</script>