<div class="table-responsive d-none d-md-block">	@if (Model.searchRecord.Count > 0)
	{		<table class="table">			<thead>				<tr>					<th scope="col">Patient Name</th>					<th scope="col">Requestor</th>					<th width="20%">Date of Service</th>					<th scope="col">Close Case Date</th>					<th scope="col">Email</th>					<th scope="col">Phone Number</th>					<th scope="col">Address</th>					<th scope="col">Zip</th>					<th scope="col">Request Status</th>					<th scope="col">Physician</th>					<th scope="col">Physician Notes</th>					<th scope="col">Cancelled By Provider Note</th>					<th scope="col">Admin Note</th>					<th scope="col">Patient Note</th>					<th scope="col">Delete Permanentaly</th>				</tr>			</thead>			<tbody>				@foreach (var obj in Model.searchRecord)
				{					<tr>						<td scope="row">@obj.PatientName</td>						<td>@obj.Requestor</td>						<td width="20%">@obj.DateOfService</td>						<td>@obj.CloseCaseDate</td>						<td>@obj.Email</td>						<td>@obj.PhoneNumber</td>						<td>@obj.Address</td>						<td>@obj.Zip</td>						<td>							@if (obj.RequestStatus == "1")							{
								<text>New</text>
							}							else if (obj.RequestStatus == "2")
							{								<text>Pending</text>
							}							else if (obj.RequestStatus == "4" || obj.RequestStatus == "5")							{
								<text>Active</text>
							}							else if(obj.RequestStatus == "6")
							{
								<text>Conclude</text>
							}							else if (obj.RequestStatus == "3" || obj.RequestStatus == "7" || obj.RequestStatus == "8")
							{
								<text>To Close</text>
							}							else if (obj.RequestStatus == "9")
							{
								<text>Unpaid</text>
							}						</td>						<td>@obj.Physician</td>						<td>@obj.PhysicianNotes</td>						<td>---</td>						<td>@obj.AdminNotes</td>						<td>@obj.PatientNotes</td>						<td>							<button class="btn btn-outline-info">Delete</button>						</td>					</tr>				}			</tbody>		</table>	}
	else
	{		<p>No Record Found Related Your Search</p>	}</div><nav aria-label="Page navigation example " class="pb-1">	<div class="justify-content-between d-flex">		@if ((Model.skipCount + Model.Page.itemperpage) >= Model.Page.totalitem)
		{			<p style="margin:0px; color:gray;">@(Model.skipCount + 1) to @Model.Page.totalitem records out of @Model.Page.totalitem</p>		}
		else
		{			<p style="margin:0px; color:gray;">@(Model.skipCount + 1) to @(Model.skipCount + Model.Page.itemperpage) records out of @Model.Page.totalitem</p>		}		<ul class="pagination">			<li class="page-item @(Model.CurrentPage == 1 ? "disabled" : "")" onclick="checkAndPaginate(@(Model.CurrentPage - 1))">				<button class="page-link" id="prevPageLink">Previous</button>			</li>			@for (int i = 1; i <= Model.TotalPages; i++)			{				<li class="page-item @(Model.CurrentPage == i ? "active" : "")" onclick="searchrecord(@i)">					<button class="page-link" id="pageLink">@i</button>				</li>			}			<li class="page-item @(Model.CurrentPage == Model.TotalPages ? "disabled" : "")" onclick="checkMaxPage(@Model.CurrentPage + 1)">				<button class="page-link" id="nextPageLink">Next</button>			</li>		</ul>		<input type="text" hidden value="@Model.TotalPages" id="maxpages" />	</div></nav><script>	function checkMaxPage(pageNumber) {		var maxpage = document.getElementById("maxpages");		console.log(maxpage);		if (pageNumber < maxpage) {			searchrecord(pageNumber);		}	}</script>