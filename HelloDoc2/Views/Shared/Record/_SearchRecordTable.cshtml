<div class="table-responsive d-none d-md-block">	@if (Model.searchRecord.Count > 0)
	{		<table class="table">			<thead>				<tr>					<th scope="col">Patient Name</th>					<th scope="col">Requestor</th>					<th width="20%">Date of Service</th>					<th scope="col">Close Case Date</th>					<th scope="col">Email</th>					<th scope="col">Phone Number</th>					<th scope="col">Address</th>					<th scope="col">Zip</th>					<th scope="col">Request Status</th>					<th scope="col">Physician</th>					<th scope="col">Physician Notes</th>					<th scope="col">Cancelled By Provider Note</th>					<th scope="col">Admin Note</th>					<th scope="col">Patient Note</th>					<th scope="col">Delete Permanentaly</th>				</tr>			</thead>			<tbody>				@foreach (var obj in Model.searchRecord)
				{					<tr>						<td scope="row">@obj.PatientName</td>						<td>@obj.Requestor</td>						<td width="20%">@obj.DateOfService</td>						<td>@obj.CloseCaseDate</td>						<td>@obj.Email</td>						<td>@obj.PhoneNumber</td>						<td>@obj.Address</td>						<td>@obj.Zip</td>						<td>							@if (obj.RequestStatus == "1")							{
								<text>New</text>
							}							else if (obj.RequestStatus == "2")
							{								<text>Pending</text>
							}							else if (obj.RequestStatus == "4" || obj.RequestStatus == "5")							{
								<text>Active</text>
							}							else if(obj.RequestStatus == "6")
							{
								<text>Conclude</text>
							}							else if (obj.RequestStatus == "3" || obj.RequestStatus == "7" || obj.RequestStatus == "8")
							{
								<text>To Close</text>
							}							else if (obj.RequestStatus == "9")
							{
								<text>Unpaid</text>
							}						</td>						<td>@obj.Physician</td>						<td>@obj.PhysicianNotes</td>						<td>---</td>						<td>@obj.AdminNotes</td>						<td>@obj.PatientNotes</td>						<td>							<button class="btn btn-outline-info deletebutton" data-requestId="@obj.RequestId">Delete</button>						</td>					</tr>				}			</tbody>		</table>	}
	else
	{		<p>No Record Found </p>	}</div><!-- Include SweetAlert --><script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>@* accordian *@<div class="d-block d-md-none my-3">	<div class="accordion accordion-flush" id="searchrecordsAccordion">		@foreach (var item in Model.searchRecord)
		{			<div class="accordion-item">				<h2 class="accordion-header" id="heading_@item.RequestId">					<button class="accordion-button collapsed rounded-2" type="button" data-bs-toggle="collapse" data-bs-target="#contentaccordian_@item.RequestId" aria-expanded="false" aria-controls="content" style="background-color: #E6faff;">						<div class="d-flex justify-content-start align-items-center">							<div>								<p class="mb-0" style="font-weight: 600;">@item.PatientName</p>								<br />								<p class="mb-0" style="color: gray; font-size: 12px;"> <span style="font-weight: 600;">@item.Address</span></p>							</div>						</div>					</button>				</h2>				<div id="contentaccordian_@item.RequestId" class="accordion-collapse collapse" style="background:#E6faff" aria-labelledby="heading_@item.RequestId" data-bs-parent="#searchrecordsAccordion">					<div class="accordion-body">						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-person text-info"></i>							<span>Requestor : @item.Requestor</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-calendar-week text-info"></i>							<span>Date Of Service : @item.DateOfService</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-calendar-week text-info"></i>							<span>Case Closed Date : @item.CloseCaseDate</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Email : @item.Email</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-envelope text-info"></i>							<span>Phone : @item.PhoneNumber</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Address : @item.Address</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Zip Code : @item.Zip</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>								Request Status :								@if (item.RequestStatus == "1")
								{									<text>New</text>								}
								else if (item.RequestStatus == "2")
								{									<text>Pending</text>								}
								else if (item.RequestStatus == "4" || item.RequestStatus == "5")
								{									<text>Active</text>								}
								else if (item.RequestStatus == "6")
								{									<text>Conclude</text>								}
								else if (item.RequestStatus == "3" || item.RequestStatus == "7" || item.RequestStatus == "8")
								{									<text>To Close</text>								}
								else if (item.RequestStatus == "9")
								{									<text>Unpaid</text>								}							</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Provider : @item.Physician</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Provider Note : @item.PhysicianNotes</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Cancelled By Provider Note : ---- </span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Admin Note : @item.AdminNotes</span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Cancellation Reasons : -- </span>						</div>						<div class="d-flex align-items-center gap-3 mb-3">							<i class="bi bi-check-circle text-info"></i>							<span>Patient Notes : @item.PatientNotes</span>						</div>					</div>				</div>			</div>		}	</div></div>@* accordian end *@@* pagination *@<nav aria-label="Page navigation example " class="pb-1">	<div class="justify-content-between d-flex">		@if ((Model.skipCount + Model.Page.itemperpage) >= Model.Page.totalitem)
		{			<p style="margin:0px; color:gray;">@(Model.skipCount + 1) to @Model.Page.totalitem records out of @Model.Page.totalitem</p>		}
		else
		{			<p style="margin:0px; color:gray;">@(Model.skipCount + 1) to @(Model.skipCount + Model.Page.itemperpage) records out of @Model.Page.totalitem</p>		}		<ul class="pagination">			<li class="page-item @(Model.CurrentPage == 1 ? "disabled" : "")" onclick="checkAndPaginate(@(Model.CurrentPage - 1))">				<button class="page-link" id="prevPageLink">Previous</button>			</li>			@for (int i = 1; i <= Model.TotalPages; i++)			{				<li class="page-item @(Model.CurrentPage == i ? "active" : "")" onclick="searchrecord(@i)">					<button class="page-link" id="pageLink">@i</button>				</li>			}			<li class="page-item @(Model.CurrentPage == Model.TotalPages ? "disabled" : "")" onclick="checkMaxPage(@Model.CurrentPage + 1)">				<button class="page-link" id="nextPageLink">Next</button>			</li>		</ul>		<input type="text" hidden value="@Model.TotalPages" id="maxpages" />	</div></nav>@* pagination end *@<script>	function checkMaxPage(pageNumber) {		var maxpage = document.getElementById("maxpages");		console.log(maxpage);		if (pageNumber < maxpage) {			searchrecord(pageNumber);		}	};	var deletebtns = document.querySelectorAll('.deletebutton');	deletebtns.forEach(function (deletebtn) {		deletebtn.addEventListener('click', function (event) {			event.preventDefault();			console.log('Accept button clicked');			var requestId = event.target.getAttribute('data-requestId');			console.log(requestId);			Swal.fire({				title: 'Are you sure?',				text: "Are you Sure You Want to Delete this request!",				icon: 'warning',				showCancelButton: true,				confirmButtonColor: '#E72929',				cancelButtonColor: '#3085d6',				confirmButtonText: 'Yes, Delete it!'			}).then((result) => {				if (result.isConfirmed) {					window.location.href = '/Admin/DeleteSearchRecord?requestId=' + requestId;				}			});		});	});</script>